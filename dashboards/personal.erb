    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="performance" data-view="Meter" data-title="Verrechenbarkeit" data-height="200" data-width="200" data-min="0" data-max="100" data-suffix="%"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="ruby_stable" data-view="Text" data-title="Ruby"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="rails_versions" data-view="Text" data-title="Rails"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="temperature_outside" data-view="Number" data-title="Temperatur"></div>
    </li>
  </ul>
</div>
